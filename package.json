{
  "name": "@atsumo/voy",
  "version": "0.1.0",
  "module": "src/index.tsx",
  "type": "module",
  "bin": {
    "voy": "dist/voy.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "bun --watch run src/index.tsx",
    "build": "bun build src/index.tsx --target bun --outfile dist/voy.js",
    "build:compile": "bun build --compile src/index.tsx --outfile voy",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "react-devtools-core": "^7.0.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "fullscreen-ink": "^0.1.0",
    "ink": "^6.8.0",
    "react": "^19.2.4"
  }
}
